{
  "name": "@ladesa-ro/web.sisgha",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "debug": "nuxt dev --inspect",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare:vendors": "wireit",
    "postinstall": "wireit",
    "start": "node .output/server/index.mjs",
    "format:check": "prettier --cache --ignore-unknown \"**/*\" --check",
    "format:write": "prettier --cache --ignore-unknown \"**/*\" --write"
  },
  "wireit": {
    "prepare:vendors": {
      "dependencies": [
        "../../libs/vue-components:build"
      ]
    },
    "postinstall": {
      "command": "nuxt prepare",
      "dependencies": [
        "prepare:vendors"
      ]
    },
    "dev": {
      "command": "nuxt dev",
      "dependencies": [
        "prepare:vendors"
      ]
    },
    "build": {
      "command": "nuxt build",
      "dependencies": [
        "prepare:vendors"
      ]
    }
  },
  "devDependencies": {
    "@auth/core": "^0.37.3",
    "@ladesa-ro/api-client-fetch": "1.0.0-next.29",
    "@ladesa-ro/vue.components": "workspace:^",
    "@mdi/font": "^7.4.47",
    "@nuxt/devtools": "^1.6.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/critters": "^0.7.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@sidebase/nuxt-auth": "1.0.0-alpha.0",
    "@tanstack/query-persist-client-core": "^5.59.20",
    "@tanstack/query-sync-storage-persister": "^5.59.20",
    "@tanstack/vue-query": "^5.59.20",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.13",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^11.2.0",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "html2pdf.js": "^0.10.2",
    "inversify": "^6.0.3",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^3.14.159",
    "openid-client": "^6.1.3",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.6.3",
    "vee-validate": "^4.14.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.1.10",
    "vue3-carousel": "^0.5.0",
    "vue3-carousel-nuxt": "^1.1.3",
    "vue3-dropzone": "^2.2.1",
    "vuetify": "^3.7.4",
    "vuetify-nuxt-module": "^0.18.3",
    "yup": "^1.4.0"
  },
  "overrides": {
    "@sidebase/nuxt-auth": {
      "@auth/core": "$@auth/core"
    }
  }
}
