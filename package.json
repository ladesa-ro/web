{
	"name": "nuxt-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"prepare:api": "rimraf ./infrastructure/api/generated; openapi --input https://luna.sisgha.com/api/doc-api-json --output ./infrastructure/api/generated --client axios --indent 2; prettier --cache --ignore-unknown \"./infrastructure/api/generated/**/*\" --write",
		"postinstall": "nuxt prepare",
		"start": "node .output/server/index.mjs",
		"format:check": "prettier --cache --ignore-unknown \"**/*\" --check",
		"format:write": "prettier --cache --ignore-unknown \"**/*\" --write"
	},
	"devDependencies": {
		"@mdi/font": "^7.3.67",
		"@nuxt/devtools": "latest",
		"@nuxtjs/tailwindcss": "^6.12.0",
		"@sidebase/nuxt-auth": "^0.7.2",
		"@sisgea/api-client": "^1.1.0-development.3",
		"@sisgea/spec": "^1.1.0-development.61",
		"@tanstack/vue-query": "^5.36.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@vueuse/core": "^10.9.0",
		"@vueuse/nuxt": "^10.9.0",
		"axios": "^1.6.7",
		"inversify": "^6.0.2",
		"jsonwebtoken": "^9.0.2",
		"next-auth": "4.21.1",
		"nuxt": "^3.10.3",
		"openapi-typescript-codegen": "^0.29.0",
		"openid-client": "^5.6.5",
		"prettier": "^3.2.5",
		"reflect-metadata": "^0.2.1",
		"vee-validate": "^4.12.8",
		"vue": "^3.4.27",
		"vue-router": "^4.3.2",
		"vue3-carousel": "^0.3.3",
		"vue3-carousel-nuxt": "^1.1.1",
		"vue3-dropzone": "^2.2.1",
		"vuetify": "^3.6.5",
		"vuetify-nuxt-module": "^0.14.0",
		"yup": "^1.4.0"
	},
	"overrides": {
		"@sidebase/nuxt-auth": {
			"next-auth": "4.21.1"
		}
	}
}
